{
  "name": "natura-orders",
  "version": "0.1.0",
  "private": true,
  "author": "Vizir Software Studio",
  "dependencies": {
    "apollo-cache-inmemory": "^1.0.0",
    "apollo-client": "^2.0.1",
    "apollo-link-http": "^1.0.0",
    "autoprefixer": "7.1.2",
    "babel-core": "6.25.0",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.1.1",
    "babel-preset-react-app": "^3.0.3",
    "babel-runtime": "6.26.0",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "1.1.3",
    "cors": "^2.8.4",
    "css-loader": "0.28.4",
    "directory-named-webpack-plugin": "^2.2.3",
    "dotenv-safe": "^4.0.4",
    "dotenv-webpack": "^1.5.4",
    "eslint": "4.4.1",
    "eslint-config-react-app": "^2.0.1",
    "eslint-loader": "1.9.0",
    "eslint-plugin-flowtype": "2.35.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jsx-a11y": "5.1.1",
    "eslint-plugin-react": "7.1.0",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "3.0.0",
    "file-loader": "0.11.2",
    "formik": "^0.11.0-alpha.2",
    "fs-extra": "3.0.1",
    "glamor": "^2.20.40",
    "graphql": "^0.11.7",
    "graphql-tag": "^2.5.0",
    "html-webpack-plugin": "2.29.0",
    "jest": "20.0.4",
    "natura-ui": "git+ssh://git@bitbucket.org/myvizir/natura-ui.git#0.2.0",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
    "postcss-loader": "2.0.6",
    "promise": "8.0.1",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-apollo": "^2.0.0",
    "react-dev-utils": "^4.1.0",
    "react-dom": "^16.0.0",
    "react-image": "^1.3.1",
    "react-infinite-scroller": "^1.1.1",
    "react-intl": "^2.4.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "style-loader": "0.18.2",
    "styled-components": "^2.2.3",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.5.9",
    "url-search-params-polyfill": "^2.0.1",
    "validate.js": "^0.12.0",
    "webpack": "3.5.1",
    "webpack-dev-server": "2.8.2",
    "webpack-manifest-plugin": "1.2.1",
    "whatwg-fetch": "2.0.3"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "build:hml": "cp .env.dev .env && node scripts/build.js",
    "build:staging": "cp .env.staging .env && node scripts/build.js",
    "build:prod": "cp .env.prod .env && node scripts/build.js",
    "test": "node scripts/test.js --env=jsdom",
    "coverage": "node scripts/test.js --env=jsdom --coverage",
    "coverage:upload": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "coverage:open": "open coverage/lcov-report/index.html",
    "transpile": "NODE_ENV=production babel src --out-dir transpiled",
    "start:prod": "NODE_ENV=production node server.js",
    "lint": "eslint src",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.{js,jsx}": [
      "eslint --fix",
      "prettier --write --single-quote --trailing-comma all --print-width 100",
      "git add"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-material-ui": "^0.3.1",
    "babel-plugin-styled-components": "^1.3.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "coveralls": "^3.0.0",
    "enzyme": "^3.2.0",
    "enzyme-adapter-react-16": "^1.1.0",
    "enzyme-to-json": "^3.2.2",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-prettier": "^2.3.1",
    "husky": "^0.14.3",
    "jest-localstorage-mock": "^2.0.1",
    "jest-styled-components": "^4.9.0",
    "lint-staged": "^4.3.0",
    "prettier": "^1.7.4",
    "raf": "^3.4.0",
    "react-test-renderer": "^16.0.0",
    "svg-react-loader": "^0.4.5",
    "svgo": "^1.0.3",
    "svgo-loader": "^2.0.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/src/index.js"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js",
      "jest-localstorage-mock",
      "raf/polyfill",
      "<rootDir>/config/jest/enzyme.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "resolver": "<rootDir>/config/jest/directoryNamedResolve.js"
  }
}
