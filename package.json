    "react-router-dom": "~4.2.2",
    "react-slick": "~0.16.0",
    "react-tippy": "~1.2.2",
    "redux": "~3.7.2",
    "style-loader": "0.18.2",
    "styled-components": "~2.2.3",
    "sw-precache-webpack-plugin": "0.11.4",
    "url-loader": "0.5.9",
    "url-search-params-polyfill": "~2.0.1",
    "validate.js": "~0.12.0",
    "video-react": "~0.9.4",
    "victory": "~0.25.6",
