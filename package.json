    "material-ui-fullscreen-dialog": "~0.7.2",
    "moment": "~2.20.1",
    "natura-ui": "git+https://igor_edeploy@bitbucket.org/naturacode/natura-fv-ui.git#dev",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.2.0",
